mergesort: libgoodmalloc.a mergesort.cpp goodmalloc.cpp goodmalloc.h 
	g++ -g mergesort.cpp -L. -lgoodmalloc -o mergesort

run: mergesort
	./mergesort

run-quiet: mergesort
	./mergesort 2> /dev/null

libgoodmalloc.a: goodmalloc.o
	ar rcs libgoodmalloc.a goodmalloc.o

goodmalloc.o: goodmalloc.h goodmalloc.cpp 
	g++ -c -g goodmalloc.cpp

clean:
	-rm goodmalloc.o

distclean:
	-rm goodmalloc.o mergesort libgoodmalloc.a